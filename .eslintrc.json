{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "rules": {
      "react/no-unescaped-entities":0,
      "react/prop-types": 0,
      "semi": ["error", "always"],      
      "quotes": ["error", "double"]
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "globals": {
      "page": "readonly"
    }
  }
  